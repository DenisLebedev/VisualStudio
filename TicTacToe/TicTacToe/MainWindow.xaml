<Window x:Class="TicTacToe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToe"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="newGame" Content="New Game" HorizontalAlignment="Left" Height="24" Margin="203,184,0,0" VerticalAlignment="Top" Width="90" Click="newGame_Click"/>
        <Button x:Name="rules" Content="Rules" HorizontalAlignment="Left" Height="24" Margin="203,213,0,0" VerticalAlignment="Top" Width="90"/>
        <Button x:Name="load" Content="Load" HorizontalAlignment="Left" Height="24" Margin="203,242,0,0" VerticalAlignment="Top" Width="90"/>
        <Button x:Name="exit" Content="Exit" HorizontalAlignment="Left" Height="24" Margin="203,271,0,0" VerticalAlignment="Top" Width="90"/>
    </Grid>
</Window>
